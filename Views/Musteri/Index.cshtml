@using MVCStarter.Models.Entity
@model List<TBLMUSTERILER> 
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/_MainLayout.cshtml";
}
<table class="table table-bordered">
    <tr>
        <th>Customer ID</th>
        <th>Customer Name</th>
        <th>Customer Surname</th>
    </tr>
    <tbody>
        @foreach (var cstm in Model)
        {
            <tr>
                <td>@cstm.MUSTERIID</td>
                <td>@cstm.MUSTERIAD</td>
                <td>@cstm.MUSTERISOYAD</td>
            </tr>
        }
    </tbody>
</table>

